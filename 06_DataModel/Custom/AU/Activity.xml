<DataObject name="Activity">
	<Key>@RefId</Key>
	<EventsReported>true</EventsReported>
	<Item>
		<Element>Activity</Element>
		<Description>
			<p>
        The work assigned to students, an educational event planned by a teacher to deliver a particular educational concept
        or skill.
      </p>
		</Description>
	</Item>
	<Item>
		<Attribute>RefId</Attribute>
		<Characteristics>M</Characteristics>
		<Description/>
		<Type ref="CommonTypes" name="RefIdType"/>
	</Item>
	<Item>
		<Element>Title</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>Descriptive name of the activity.</p>
		</Description>
		<Type name="xs:normalizedString"/>
	</Item>
	<Item>
		<Element>Preamble</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>Description and context for the activity.</p>
		</Description>
		<Type name="xs:string"/>
	</Item>
	<Item>
		<Element>TechnicalRequirements</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>List of Operating system, network, and other technical requirements for the activity, if applicable.</p>
		</Description>
		<!--<Type ref="DataModel" name="TechnicalRequirements"/>-->
		<List mode="List"></List>
	</Item>
	<Item>
		<Element>/TechnicalRequirement</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>an operating system, network, or other technical requirement.</p>
		</Description>
		<Type name="xs:string"/>
	</Item>
	<Item>
		<Element>SoftwareRequirementList</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>Contains information about required software.</p>
		</Description>
		<!--<Type ref="DataModel" name="SoftwareRequirementList"/> -->
		<List mode="List"></List>
	</Item>
	
	
	<Item>
		<Element>/SoftwareRequirement</Element>
		<Characteristics>OR</Characteristics>
		<Description>
		  <p>Contains information about a specific required piece of software.</p>
		</Description>

	</Item>

	<Item>
		<Element>//SoftwareTitle</Element>
		<Characteristics>M</Characteristics>
		<Description>
		  <p>This is the title of the specific software.  This does not include the vendor.  For example, Flash not Adobe.</p>
		</Description>
		<Type name='xs:string'/>
	</Item>

	<Item>
		<Element>//Version</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>This includes the version of the specific software.</p>
		</Description>
		<Type name='xs:string'/>
	</Item>


	<Item>
		<Element>//Vendor</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>This is the vendor of the specific software.  For example, Adobe.</p>
		</Description>
		<Type name='xs:string'/>
	</Item>

	<Item>
		<Element>//OS</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>This is the operating system and version.  For example, Macintosh OS X 1.4.</p>
		</Description>
		<Type name='xs:string'/>
	</Item>
	<Item>
		<Element>EssentialMaterials</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>Essential materials required to engage in this activity.</p>
		</Description>
		<List mode="List"/>
	</Item>
	<Item>
		<Element>/EssentialMaterial</Element>
		<Characteristics>OR</Characteristics>
		<Description>
			<p>Essential material required to engage in this activity.</p>
		</Description>
		<Type name="xs:string"/>
	</Item>
	<Item>
		<Element>LearningObjectives</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>Educational objectives of the activity.</p>
		</Description>
		<List mode="List"/>
	</Item>
	<Item>
		<Element>/LearningObjective</Element>
		<Characteristics>OR</Characteristics>
		<Description>
			<p>Description of an educational objective of the activity.</p>
		</Description>
		<Type name="xs:string"/>
	</Item>
	<Item>
		<Element>LearningStandards</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>
        Associated <code>LearningStandardItem</code> objects.
      </p>
		</Description>
		<List mode="List"/>
	</Item>
	<Item>
		<Element>/LearningStandardItemRefId</Element>
		<Characteristics>OR</Characteristics>
		<Description>
			<p>Link from activity to associated LearningStandardItem object.</p>
		</Description>
		<Type ref="CommonTypes" name="IdRefType"/>
	</Item>
	<Item>
		<Element>SubjectArea</Element>
		<Characteristics>O</Characteristics>
		<Type ref="DataModel" name="SubjectAreaType"/>
	</Item>
	<Item>
		<Element>Prerequisites</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>Skills or competencies the student must have to engage in activity.</p>
		</Description>
		<List mode="List"/>
	</Item>
	<Item>
		<Element>/Prerequisite</Element>
		<Characteristics>OR</Characteristics>
		<Description>
			<p>Skill or competency the student must have to engage in activity.</p>
		</Description>
		<Type name="xs:string"/>
	</Item>
	<Item>
		<Element>Students</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>
        Links to any students or group or category of students that this activity is for.
      </p>
		</Description>
		<List mode="List"/>
	</Item>
	<Item>
		<Element>/StudentPersonalRefId</Element>
		<Characteristics>OR</Characteristics>
		<Description>
			<p>Link to a student.</p>
		</Description>
		<Type ref="CommonTypes" name="IdRefType"/>
	</Item>
	<Item>
		<Element>SourceObjects</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>
        Source objects assigned to students.
      </p>
		</Description>
		<List mode="List"/>
	</Item>
	<Item>
		<Element>/SourceObject</Element>
		<Characteristics>OR</Characteristics>
		<Description>
			<p>GUID assigned to the source object.</p>
		</Description>
		<Type ref="CommonTypes" name="IdRefType"/>
	</Item>
	<Item>
		<Attribute>SIF_RefObject</Attribute>
		<Characteristics>M</Characteristics>
		<Description>
			<p>The type of learning object the source object is.</p>
		</Description>
		<Values>
			<Value>
				<Code>Assessment</Code>
			</Value>
			<Value>
				<Code>LearningResource</Code>
			</Value>
			<Value>
				<Code>Activity</Code>
			</Value>
			<Value>
				<Code>Lesson</Code>
			</Value>
		</Values>
	</Item>
	<Item>
		<Element>Points</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>Number of possible points for an activity.</p>
		</Description>
		<Type name="xs:unsignedInt"/>
	</Item>
	<Item>
		<Element>ActivityTime</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>Time structures for the activity.</p>
		</Description>
	</Item>
	<Item>
		<Element>/CreationDate</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>Creation date of the activity.</p>
		</Description>
		<Type name="xs:date"/>
	</Item>
	<Item>
		<Element>/Duration</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>Time required to complete the activity.</p>
		</Description>
		<Type name="xs:unsignedInt"/>
	</Item>
	<Item>
		<Attribute>Units</Attribute>
		<Characteristics>M</Characteristics>
		<Description>
			<p>Unit of time of the Duration value.&#160;</p>
		</Description>
		<Values>
			<Value>
				<Code>week</Code>
			</Value>
			<Value>
				<Code>day</Code>
			</Value>
			<Value>
				<Code>hour</Code>
			</Value>
			<Value>
				<Code>minute</Code>
			</Value>
			<Value>
				<Code>second</Code>
			</Value>
		</Values>
	</Item>
	<Item>
		<Element>/StartDate</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>Date activity is started.</p>
		</Description>
		<Type name="xs:date"/>
	</Item>
	<Item>
		<Element>/FinishDate</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>Date activity is finished.</p>
		</Description>
		<Type name="xs:date"/>
	</Item>
	<Item>
		<Element>/DueDate</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>Date activity is due.</p>
		</Description>
		<Type name="xs:date"/>
	</Item>
	<Item>
		<Element>AssessmentRefId</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>Link to the SIF Assessment object containing the evaluation information for this activity.</p>
		</Description>
		<Type ref="CommonTypes" name="IdRefType"/>
	</Item>
	<Item>
		<Element>MaxAttemptsAllowed</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>How many tries allowed.</p>
		</Description>
		<Type name="xs:unsignedInt"/>
	</Item>
	<Item>
		<Element>ActivityWeight</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>The percentage weight of the activity during the particular course or term.</p>
		</Description>
		<Type name="xs:decimal"/>
	</Item>
	<Item>
		<Element>Evaluation</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>Field for non-numeric types of grading and evaluation such as "narrative."</p>
		</Description>
	</Item>
	<Item>
		<Attribute>EvaluationType</Attribute>
		<Characteristics>M</Characteristics>
		<Description>
			<p>A qualifer for what type of evaluation this is. Valid values are "Inline" and "RefId". Inline values are contained in the optional Description element, RefIds are contained in the AssessmentRefId element.</p>
		</Description>
		<Values>
			<Value>
				<Code>Inline</Code>
			</Value>
			<Value>
				<Code>RefId</Code>
			</Value>
		</Values>
	</Item>
	<Item>
		<Element>/Description</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>A free text description of the evaluation to be used for this activity</p>
		</Description>
		<Type name="xs:string"/>
	</Item>
	<Item>
		<Element>LearningResources</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>Learning resources that support this activity.</p>
		</Description>
		<List mode="List"/>
	</Item>
	<Item>
		<Element>/LearningResourceRefId</Element>
		<Characteristics>OR</Characteristics>
		<Description>
			<p>A GUID reference to a SIF LearningResource object that supports this activity</p>
		</Description>
		<Type ref="CommonTypes" name="IdRefType"/>
	</Item>
	<Item>
		<Element>SIF_Metadata</Element>
		<Characteristics>O</Characteristics>
		<Type ref="DataModel" name="SIF_MetadataType"/>
	</Item>
	<Item>
		<Element>SIF_ExtendedElements</Element>
		<Characteristics>O</Characteristics>
		<Type ref="DataModel" name="SIF_ExtendedElementsType"/>
	</Item>
	<xhtml:Example name="Activity - Example 1" xmlns="">
		<Activity RefId="C27E1FCF-C163-485F-BEF0-F36F18A0493A"  xml:lang="en">
			<Title>Shakespeare Essay - Much Ado About Nothing</Title>
			<Preamble>This is a very funny comedy - students should have passing familiarity with Shakespeare</Preamble>
			<LearningStandards>
				<LearningStandardItemRefId>9DB15CEA-B2C5-4F66-94C3-7D0A0CAEDDA4</LearningStandardItemRefId>
			</LearningStandards>
			<SourceObjects>
				<SourceObject SIF_RefObject="Lesson">A71ADBD3-D93D-A64B-7166-E420D50EDABC</SourceObject>
			</SourceObjects>
			<Points>50</Points>
			<ActivityTime>
				<CreationDate>2002-06-15</CreationDate>
				<Duration Units="minute">30</Duration>
				<StartDate>2002-09-10</StartDate>
				<FinishDate>2002-09-12</FinishDate>
				<DueDate>2002-09-12</DueDate>
			</ActivityTime>
			<AssessmentRefId>03EDB29E-8116-B450-0435-FA87E42A0AD2</AssessmentRefId>
			<MaxAttemptsAllowed>3</MaxAttemptsAllowed>
			<ActivityWeight>5</ActivityWeight>
			<Evaluation EvaluationType="Inline">
				<Description>Students should be able to correctly identify all major characters.</Description>
			</Evaluation>
			<LearningResources>
				<LearningResourceRefId>B7337698-BF6D-B193-7F79-A07B87211B93</LearningResourceRefId>
			</LearningResources>
		</Activity>
	</xhtml:Example>
	<xhtml:Example name="Activity - Example 2" xmlns="">
		<Activity RefId="C27E1FCF-C163-485F-BEF0-F36F18A0493A" xml:lang="en">
			<Title>Shakespeare Essay - Much Ado About Nothing</Title>
			<Preamble>This is a very funny comedy - students should have passing familiarity with Shakespeare</Preamble>
			<SoftwareRequirementList>
				<SoftwareRequirement>
					<SoftwareTitle>Flash Player</SoftwareTitle>
					<Version>9.0</Version>
					<Vendor>Adobe</Vendor>
				</SoftwareRequirement>
				<SoftwareRequirement>
					<SoftwareTitle>Python</SoftwareTitle>
					<Version>3.0</Version>
					<OS>Linux</OS>
				</SoftwareRequirement>
			</SoftwareRequirementList>
			<LearningStandards>
				<LearningStandardItemRefId>9DB15CEA-B2C5-4F66-94C3-7D0A0CAEDDA4</LearningStandardItemRefId>
			</LearningStandards>
			<SourceObjects>
				<SourceObject SIF_RefObject="Lesson">A71ADBD3-D93D-A64B-7166-E420D50EDABC</SourceObject>
			</SourceObjects>
			<Points>50</Points>
			<ActivityTime>
				<CreationDate>2002-06-15</CreationDate>
				<Duration Units="minute">30</Duration>
				<StartDate>2002-09-10</StartDate>
				<FinishDate>2002-09-12</FinishDate>
				<DueDate>2002-09-12</DueDate>
			</ActivityTime>
			<AssessmentRefId>03EDB29E-8116-B450-0435-FA87E42A0AD2</AssessmentRefId>
			<MaxAttemptsAllowed>3</MaxAttemptsAllowed>
			<ActivityWeight>5</ActivityWeight>
			<Evaluation EvaluationType="Inline">
				<Description>Students should be able to correctly identify all major characters.</Description>
			</Evaluation>
			<LearningResources>
				<LearningResourceRefId>B7337698-BF6D-B193-7F79-A07B87211B93</LearningResourceRefId>
			</LearningResources>
		</Activity>
	</xhtml:Example></DataObject>
