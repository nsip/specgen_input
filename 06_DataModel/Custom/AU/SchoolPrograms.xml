<DataObject name='SchoolPrograms'>
  <Intro>
    This object contains information about Programs offered by a school.
  </Intro>
  <EventsReported>true</EventsReported>
  <Key>@RefId</Key>
  <Item>
    <Element>SchoolPrograms</Element>
    <Description>
      This object contains information about Programs offered by a school.
    </Description>
  </Item>
  <Item>
    <Attribute>RefId</Attribute>
    <Description>GUID that uniquely identifies this instance of the object.</Description>
    <Characteristics>M</Characteristics>
    <Type ref='CommonTypes' name='RefIdType'/>
  </Item>

  <Item>
    <Element>SchoolInfoRefId</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>GUID that identifies the school offering this program.</p>
    </Description>
    <Type ref='CommonTypes' name='IdRefType'/>
  </Item>

  <Item>
    <Element>SchoolYear</Element>

    <Characteristics>O</Characteristics>
    <Description>
      School year for which the information is applicable, expressed as the four-digit year in which the school year ends (e.g., "2009").
    </Description>
    <Type ref="DataModel" name="SchoolYear"></Type>
  </Item>

  <Item>
    <Element>SchoolProgramList</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>List of School Programs</p>
    </Description>
    <List mode='List'/>
  </Item>

  <Item>
    <Element>/Program</Element>
    <Characteristics>OR</Characteristics>
    <Description>
      <p></p>
    </Description>
  </Item>

  <Item>
    <Element>//Category</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Program Category</p>
    </Description>
    <Type name='xs:normalizedString'/>
  </Item>

  <Item>
    <Element>//Type</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Program Type</p>
    </Description>
    <Type name='xs:normalizedString'/>
  </Item>

  <Item>
    <Element>//OtherCodeList</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="OtherCodeList"></Type>
  </Item>
  
  <Item>
    <Element>SIF_Metadata</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_Metadata"></Type>
  </Item>
  <Item>
    <Element>SIF_ExtendedElements</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_ExtendedElements"></Type>
  </Item>

  <xhtml:Example xmlns=''>
    <SchoolPrograms RefId="D3E34B359D75101A8C3D00AA001A1652">
      <SchoolInfoRefId>73648462888624AA5294BC6380173276</SchoolInfoRefId>
      <SchoolYear>2009</SchoolYear>
      <SchoolProgramList>
        <Program>
          <Category>01</Category>
          <Type>Steiner program</Type>
        </Program>
        <Program>
          <Category>01</Category>
          <Type>Tournament of minds</Type>
        </Program>
      </SchoolProgramList>
    </SchoolPrograms>

  </xhtml:Example>
</DataObject>