<DataObject name="CalendarDate">

  <Key>@CalendarDateRefId</Key>
   
  <EventsReported>true</EventsReported>

  <Item>
    <Element>CalendarDate</Element>
    <Description>
      <p>This object defines information related to a school calendar day in a given school calendar year. If both CalendarDate and CalendarSummary objects are supported, there must be an instance of this object for each date between CalendarSummary StartDate and EndDate, inclusive.</p>
    </Description>
  </Item>
    <Item>
    <Attribute>CalendarDateRefId</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>An ID (GUID) for this day date of the calendar information that is being reported. This GUID would be used to form a REST URL.</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>
  <Item>
     <Element>Date</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>A specific school day that occurs within the school year.</p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>
  <Item>
    <Element>CalendarSummaryRefId</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>The ID (GUID) of the calendar summary in which this calendar date is.</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>
  <Item>
    <Element>SchoolInfoRefId</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>The ID (GUID) of the school for which this calendar information is being reported.</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>
  <Item>
    <Element>SchoolYear</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>School year for which the information is applicable, expressed as the four-digit year in which the school year ends (e.g., "2009").</p>
    </Description>
    <Type ref="DataModel" name="SchoolYearType"></Type>
  </Item>
  <Item>
    <Element>CalendarDateType</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Calendar date type information.</p>
    </Description>
  </Item>
  <Item>
    <Element>/Code</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Code indicating the type of school day.</p>
    </Description>
    
      <Type ref="CodeSets" name="AUCodeSetsCalendarEventType"></Type>
    
  </Item>
  <Item>
    <Element>/OtherCodeList</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="OtherCodeListType"></Type>
  </Item>
  <Item>
    <Element>CalendarDateNumber</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Order in which the calendar date falls within the school calendar.</p>
    </Description>
    <Type name="xs:unsignedInt"/>
  </Item>
  <Item>
    <Element>StudentAttendance</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Information describing how the day relates to student attendance.</p>
    </Description>
  </Item>
  <Item>
    <Element>/CountsTowardAttendance</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Designates whether this date should be counted toward student attendance.</p>
    </Description>
    <Values>
      <Value>
        <Code>Yes</Code>
      </Value>
      <Value>
        <Code>No</Code>
      </Value>
    </Values>
  </Item>
  <Item>
    <Element>/AttendanceValue</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>
        Amount of the school day in which the student should be in attendance (Format is x.x; a student who should be in attendance a full day would be represented as 1.0).  If <code>CountsTowardAttendance</code> equals <code>Yes</code> then <code>AttendanceValue</code> must be greater than zero.
      </p>
    </Description>
    <Type name="xs:decimal"></Type>
    <Facets>
      <xs:minInclusive value="0"/>
      <xs:maxInclusive value="1"/>
      <xs:fractionDigits value="1"/>
    </Facets>
  </Item>
  <Item>
    <Element>TeacherAttendance</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Information describing how the day relates to teacher attendance.</p>
    </Description>
  </Item>
  <Item>
    <Element>/CountsTowardAttendance</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Designates whether this date should be counted toward teacher attendance.</p>
    </Description>
    <Values>
      <Value>
        <Code>Yes</Code>
      </Value>
      <Value>
        <Code>No</Code>
      </Value>
    </Values>
  </Item>
  <Item>
    <Element>/AttendanceValue</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Amount of the school day in which the teacher should be in attendance (format is x.x; a teacher who should be in attendance a full day would be represented as 1.0).</p>
    </Description>
    <Type name="xs:decimal"></Type>
    <Facets>
      <xs:minInclusive value="0"/>
      <xs:maxInclusive value="1"/>
      <xs:fractionDigits value="1"/>
    </Facets>
  </Item>
  <Item>
    <Element>AdministratorAttendance</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Information describing how the day relates to administrator attendance.</p>
    </Description>
  </Item>
  <Item>
    <Element>/CountsTowardAttendance</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Designates whether this date should be counted toward administrator attendance.</p>
    </Description>
    <Values>
      <Value>
        <Code>Yes</Code>
      </Value>
      <Value>
        <Code>No</Code>
      </Value>
    </Values>
  </Item>
  <Item>
    <Element>/AttendanceValue</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Amount of the school day in which the administrator should be in attendance (format is x.x; an administrator who should be in attendance a full day would be represented as 1.0).</p>
    </Description>
    <Type name="xs:decimal"></Type>
    <Facets>
      <xs:minInclusive value="0"/>
      <xs:maxInclusive value="1"/>
      <xs:fractionDigits value="1"/>
    </Facets>
  </Item>
  <Item>
    <Element>SIF_Metadata</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_MetadataType"></Type>
  </Item>
  <Item>
    <Element>SIF_ExtendedElements</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_ExtendedElementsType"></Type>
  </Item>
  <xhtml:Example xmlns="">
    <CalendarDate CalendarDateRefId="B5739375-800A-C4CC-6385-0BB2754114AA">
      <Date>2007-08-31</Date>
      <CalendarSummaryRefId>A8A34E56-1C97-345C-0A4E-11BB113C2753</CalendarSummaryRefId>
      <SchoolInfoRefId>B7A34E56-1C97-345C-0A4E-11BB112B2753</SchoolInfoRefId>
      <SchoolYear>2007</SchoolYear>
      <CalendarDateType>
        <Code>INST</Code>
      </CalendarDateType>
      <StudentAttendance>
        <CountsTowardAttendance>Yes</CountsTowardAttendance>
        <AttendanceValue>1.0</AttendanceValue>
      </StudentAttendance>
      <TeacherAttendance>
        <CountsTowardAttendance>Yes</CountsTowardAttendance>
        <AttendanceValue>1.0</AttendanceValue>
      </TeacherAttendance>
    </CalendarDate>

  </xhtml:Example>
</DataObject>