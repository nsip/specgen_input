			<AdministrationAssessment>90E298F70E094EE2B8B52DFD88006AF2</AdministrationAssessment>
		  </AdministrationAssessments>
		  <Organizations>
			<Organization>
			  <LEAInfoRefId>06AF290E298F70E094EE2B8B52DFD880</LEAInfoRefId>
