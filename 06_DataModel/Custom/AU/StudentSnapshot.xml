<DataObject name="StudentSnapshot">
  <Key>@SnapDate</Key>
  <Key>@StudentPersonalRefId</Key>
  <EventsReported>false</EventsReported>
  <Item>
    <Element>StudentSnapshot</Element>
    <Description>
      <p>
        This object provides a snapshot of a student's record on a given day. All information reported in the object is
        reported as it appeared in the responding system on the date specified in SnapDate. It can be used for synching data
        across applications, for periodically loading a data warehouse, or for vertical reporting of data to a requesting
        authority, such as a state department of education.
      </p>
    </Description>
  </Item>
  <Item>

    <Attribute>SnapDate</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>
        The date the data snapshot was taken.
        Some elements are provided/calculated as of the SnapDate, and some are provided as of the date the object is requested
        (i.e., some can be historical and some will represent the last information available/last known value).
        In a SIS the following elements are provided/calculated as of the SnapDate: Age, HomeEnrollment and all its child elements.
        Other applications (e.g., data warehouses) may be able to provide values for other elements on various snap dates.
      </p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>
  <Item>

    <Attribute>StudentPersonalRefId</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>The ID (GUID) of the student to whom this information relates.</p>
    </Description>
    <Type ref="CommonTypes" relationship="is snapshot of this student's data" count="?:1" name="IdRefType"></Type>
  </Item>
  <Item>
    <Element>StudentSnapshotRefId</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>An optional ID (GUID) for this StudentSnapshot. This GUID would be used to form a REST URL.</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>
  
  <Item>
    <Element>SchoolYear</Element>

    <Characteristics>M</Characteristics>
    <Description>
      <p>
        School year in which the SnapDate falls, and for which the information is applicable, expressed as the four-digit year
        in which the school year ends (e.g. <code>2007</code>).
      </p>
    </Description>
    <Type ref="DataModel" name="SchoolYear"></Type>
  </Item>
  <Item>
    <Element>Name</Element>

    <Characteristics>M</Characteristics>
    <Description>
      <p>The name of the student.</p>
    </Description>
    <Type ref="DataModel" name="Name"></Type>
  </Item>
  <Item>
    <Element>LocalId</Element>

    <Characteristics>M</Characteristics>
    <Description>
      <p>The locally-defined identifier for this student.</p>
    </Description>
    <Type ref="DataModel" name="LocalId"></Type>
  </Item>
  <Item>
    <Element>StateProvinceId</Element>

    <Characteristics>O</Characteristics>
    <Description>
      <p>The state-assigned identifier for this student.</p>
    </Description>
    <Type ref="DataModel" name="StateProvinceId"></Type>
  </Item>



  <Item>
    <Element>Sex</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>'Sex' is the distinction 'male' and 'female', as reported by the person.</p>
    </Description>
    <Type ref="CodeSets" name="AUCodeSetsSexCodeType"></Type>
  </Item>
  <Item>
    <Element>BirthDate</Element>

    <Characteristics>O</Characteristics>
    <Description>
      <p>The person's date of birth.</p>
    </Description>
    <Type ref="DataModel" name="BirthDate"></Type>
  </Item>
  <Item>
    <Element>Age</Element>

    <Characteristics>O</Characteristics>
    <Description>
      <p>The age (in years) of the student on the date in SnapDate.</p>
    </Description>
    <Type name="xs:unsignedInt"></Type>
  </Item>
  <Item>
    <Element>ProjectedGraduationYear</Element>

    <Characteristics>O</Characteristics>
    <Description>
      <p>Currently projected graduation year.</p>
    </Description>
    <Type ref="DataModel" name="ProjectedGraduationYear"></Type>
  </Item>
  <Item>
    <Element>OnTimeGraduationYear</Element>

    <Characteristics>O</Characteristics>
    <Description>
      <p>First projected graduation year, usually determined when student is accepted into Year 9.</p>
    </Description>
    <Type ref="DataModel" name="OnTimeGraduationYear"></Type>
  </Item>

  <Item>
    <Element>StudentSubjectChoiceList</Element>

    <Characteristics>O</Characteristics>
    <Description>
      <p>List of Student Subject Choices where available.</p>
    </Description>
    <List mode="List"></List>
  </Item>

  <Item>
    <Element>/StudentSubjectChoice</Element>

    <Characteristics>MR</Characteristics>
    <Description>
      <p>Subjects Students Choose</p>
    </Description>
  </Item>

  <Item>
    <Element>//PreferenceNumber</Element>

    <Characteristics>O</Characteristics>
    <Description>
      <p>Subject Priority</p>
    </Description>
    <Type name="xs:unsignedInt"></Type>
  </Item>
  <Item>
    <Element>//SubjectLocalId</Element>

    <Characteristics>M</Characteristics>
    <Description>
      <p>Local Subject Id</p>
    </Description>
    <Type ref="DataModel" name="LocalId"></Type>
  </Item>
  <Item>
    <Element>//StudyDescription</Element>

    <Characteristics>O</Characteristics>
    <Description>
      <p>Description about Study Mode.</p>
    </Description>
    <Type ref="DataModel" name="SubjectArea"></Type>
  </Item>
  <Item>
    <Element>//OtherSchoolLocalId</Element>

    <Characteristics>O</Characteristics>
    <Description>
      <p>Localid of a school where the student studies this subject if not at the home school.</p>
    </Description>
    <Type ref="DataModel" name="LocalId"></Type>
  </Item>
  
  <Item>
    <Element>HomeEnrollment</Element>

    <Characteristics>M</Characteristics>
    <Description>
      <p>
        Enrollment-related information for the school that is responsible for reporting the student's
        membership/child accounting information. This is most likely the primary enrollment site for the student.
      </p>
    </Description>
  </Item>
  <Item>
    <Element>/StudentSchoolEnrollmentRefId</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>The ID (GUID) of the StudentSchoolEnrollment object from which the enrollment information is derived.</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"/>
  </Item>
  <Item>
    <Element>/SchoolName</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Name of the school.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>
  <Item>
    <Element>/SchoolInfoRefId</Element>
    <Characteristics>C</Characteristics>
    <Description>
      <p>
        The ID (GUID) of the school.  Provide both the HomeEnrollment/SchoolInfoRefId and HomeEnrollment/LocalId elements if possible.
        If not, one or the other must be provided.
      </p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"/>
  </Item>
  <Item>
    <Element>/LocalId</Element>
    <Characteristics>C</Characteristics>
    <Description>
      <p>
        The locally-defined identifier for this school.
        Provide both the HomeEnrollment/LocalId and HomeEnrollment/SchoolId elements if possible.
        If not, one or the other must be provided.
      </p>
    </Description>
    <Type ref="DataModel" name="LocalId"></Type>
  </Item>

  <Item>
    <Element>/SchoolNo</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>
        The state or province defined identifier for this school.
      </p>
    </Description>
    <Type ref="DataModel" name="LocalId"></Type>
  </Item>
  <Item>
    <Element>/YearLevel</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Year or academic level of student.</p>
    </Description>
    <Type ref="DataModel" name="YearLevel"></Type>
  </Item>
  <Item>
    <Element>/Homeroom</Element>
    <Description>HomeGroup this student belongs to</Description>
    <Characteristics>O</Characteristics>
    <Type ref="CommonTypes" name="IdRefType"/>
  </Item>
  <Item>
    <Attribute>SIF_RefObject</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>The name of the SIF object referenced.</p>
    </Description>
    <Values>
      <Value>
        <Code>RoomInfo</Code>
      </Value>
    </Values>
  </Item>
  <Item>
    <Element>/HomeroomNumber</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>The locally-defined identifier of that room</p>
    </Description>
    <Type ref="DataModel" name="HomeroomNumber"></Type>
  </Item>
  
 
  <Item>
    <Element>SIF_Metadata</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_Metadata"></Type>
  </Item>
  <Item>
    <Element>SIF_ExtendedElements</Element>

    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_ExtendedElements"></Type>
  </Item>
  <xhtml:Example xmlns="">
    <StudentSnapshot SnapDate="2003-10-01" StudentPersonalRefId="A15484ED564995254A4568EFFC5100BD">
      <SchoolYear>2004</SchoolYear>
      <Name Type="LGL">
        <FamilyName>Woodall</FamilyName>
        <GivenName>Charles</GivenName>
        <MiddleName>William</MiddleName>
      </Name>
      <LocalId>1089543</LocalId>
      <StateProvinceId>A50321</StateProvinceId>
      

      <Sex>1</Sex>
      <BirthDate>1988-03-02</BirthDate>
      <Age>15</Age>
      <ProjectedGraduationYear>2007</ProjectedGraduationYear>
      <OnTimeGraduationYear>2006</OnTimeGraduationYear>
      <HomeEnrollment>
        <StudentSchoolEnrollmentRefId>0847987235489729AAA73011BB365EC3</StudentSchoolEnrollmentRefId>
        <SchoolName>Academy HS</SchoolName>
        <SchoolInfoRefId>834787627836AC74B05EE3066ABC9231</SchoolInfoRefId>
        <LocalId>ACAD</LocalId>
       
        <YearLevel>
          <Code>9</Code>
        </YearLevel>
        <Homeroom SIF_RefObject="RoomInfo">9475683579BA648E0548DDA8365700F4</Homeroom>
        <HomeroomNumber>B024</HomeroomNumber>
        
      </HomeEnrollment>
      
    </StudentSnapshot>
  </xhtml:Example>
</DataObject>